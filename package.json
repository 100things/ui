{
  "name": "100things",
  "version": "1.0.0",
  "main": "dev-server.js",
  "scripts": {
    "start": "npm run dev",
    "server": "nodemon --debug --watch client --watch server --ext js,jsx dev-server.js",
    "server-dev": "webpack-dev-server --config webpack.config.dev.js --progress --colors --port 2992",
    "server-hot": "webpack-dev-server --config webpack.config.hot.js --hot --progress --colors --port 2992 --inline",
    "sources": "http-server -p 3001 .",
    "watch": "webpack --watch",
    "prod": "npm run watch & npm run server & npm run sources",
    "dev": "npm run server-dev & WEBPACK_DEV=true npm run server",
    "hot": "npm run server-hot & WEBPACK_DEV=true npm run server",
    "build": "webpack",
    "test": "karma start",
    "test-debug": "karma start --browsers=Chrome --single-run=false"
  },
  "license": "MIT",
  "dependencies": {
    "alt": "^0.17.1",
    "axios": "^0.5.4",
    "babel": "^5.6.23",
    "babel-core": "^5.7.4",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.7.0",
    "clean-webpack-plugin": "^0.1.2",
    "color": "^0.10.1",
    "config": "^1.12.0",
    "css-loader": "^0.9.0",
    "es6-promise": "^2.3.0",
    "file-loader": "^0.8.1",
    "handlebars": "^3.0.3",
    "hapi": "^8.4.0",
    "iso": "^4.1.0",
    "lodash": "^3.10.0",
    "radium": "^0.13.4",
    "react": "^0.13.2",
    "react-masonry-component": "^1.0.4",
    "react-router": "^0.13.3",
    "webpack": "^1.8.11",
    "webpack-stats-plugin": "0.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.17",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.2.0",
    "http-server": "^0.8.0",
    "isparta-loader": "^0.2.0",
    "karma": "^0.12.37",
    "karma-babel-preprocessor": "^5.2.1",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.4.2",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.3",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-phantomjs-shim": "^1.0.0",
    "karma-sinon-chai": "^1.0.0",
    "karma-webpack": "^1.6.0",
    "lowdb": "^0.10.2",
    "mocha": "^2.2.5",
    "nodemon": "^1.3.7",
    "phantomjs": "^1.9.17",
    "react-hot-loader": "^1.2.5",
    "sinon": "^1.13.0",
    "sinon-chai": "^2.8.0",
    "uuid": "^2.0.1",
    "webpack-dev-server": "^1.8.2"
  }
